<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Exclude cache and temporary files -->
    <exclude domain="cache" />
    <exclude domain="files" path="cache/"/>
    <exclude domain="files" path="models/"/>
    <!-- Include user settings and chat history -->
    <include domain="sharedpref" path="."/>
    <include domain="database" path="chat_history.db"/>
</full-backup-content>
